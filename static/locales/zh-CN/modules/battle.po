# Battle messages
msgid ""
"{winner} defeated {loser}, dealing {damage} damage. {loser} was fatally "
"wounded and perished."
msgstr "{winner} 战胜了 {loser}，造成 {damage} 点伤害。{loser} 遭受重创，当场陨落。"

msgid ""
"{winner} defeated {loser}. {loser} took {loser_dmg} damage, {winner} also "
"took {winner_dmg} damage."
msgstr ""
"{winner} 战胜了 {loser}，{loser} 受伤 {loser_dmg} 点，{winner} 也受伤 {winner_dmg} 点。"

# Battle - action descriptions
msgid "defeated"
msgstr "战胜了"

msgid "{p1} and {p2} gave birth to a {gender} named {child}."
msgstr "{p1} 与 {p2} 诞下一{gender}，取名为 {child}。"

msgid "son"
msgstr "子"

msgid "daughter"
msgstr "女"
