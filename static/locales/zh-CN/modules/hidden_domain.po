# ============================================================================
# Hidden Domain (秘境)
# ============================================================================
msgid "hidden_domain_story_prompt"
msgstr ""
"描述探索秘境的过程。可以聚焦于单人的经历，也可以描述多人间的互动（如相遇、合作、竞争甚至相互攻击）。内容应涵盖遭遇的凶险、获得的机缘，或是空手而归的失落与遗憾，营造神秘、紧张或充满变数的氛围。"

msgid "Hidden Domains opened: {names}"
msgstr "{names}"

msgid "Hidden Domain {name} opened! Entry restricted to {realm} only."
msgstr "秘境【{name}】现世！仅限{realm}修士进入。"

msgid "{name} perished in the hidden domain {domain}."
msgstr "{name} 葬身于秘境【{domain}】。"

msgid "{name} found a treasure {loot} in {domain}!"
msgstr "{name} 在秘境【{domain}】中觅得宝物【{loot}】！"

msgid "{names} returned from {domain} empty-handed."
msgstr "{names} 在秘境【{domain}】中一无所获，空手而归。"

msgid "Perished in a Hidden Domain"
msgstr "在秘境中陨落"

msgid ""
"Event: Hidden Domain Opening\n"
"Name: {name}\n"
"Description: {desc}"
msgstr ""
"事件：秘境开启\n"
"名称：{name}\n"
"描述：{desc}"

msgid ""
"Hidden Domain {name} opened! Entry restricted to {realm} only. Entrants: "
"{entrants}"
msgstr "秘境【{name}】现世！仅限{realm}修士进入。进入者：{entrants}。"

msgid ""
"Hidden Domain {name} opened! Entry restricted to {realm} only. No one "
"entered."
msgstr "秘境【{name}】现世！仅限{realm}修士进入。无修士进入。"

msgid "{}: {}"
msgstr "{}：{}"

msgid "found a treasure"
msgstr "觅得宝物"

msgid "perished"
msgstr "葬身于"
