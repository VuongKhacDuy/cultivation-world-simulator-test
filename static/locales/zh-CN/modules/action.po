msgstr "心境提升，突破概率增加 {val:.1%}"

msgid "action_reading_desc"
msgstr "研读古籍，增长见闻。"

msgid "action_tea_tasting_desc"
msgstr "品味清茶，静心养性。"

msgid "action_traveling_desc"
msgstr "游历四方，感悟天地。"

msgid "action_zither_playing_desc"
msgstr "抚琴奏乐，陶冶情操。"

msgid "action_tea_party_desc"
msgstr "与友共品清茶。"

msgid "action_chess_desc"
msgstr "对弈博弈，磨砺心智。"

msgid "action_tea_party_story_prompt"
msgstr "两人正在举行一场雅致的茶会。"

msgid "action_chess_story_prompt"
msgstr "两人正在对弈下棋。"

msgid "action_educate_short_name"
msgstr "教化"

msgid "action_meditate_short_name"
msgstr "禅定"

# Action: Meditate
msgid "meditate_action_name"
msgstr "禅定"

msgid "meditate_description"
msgstr "佛门修炼，通过坐禅平复心境，有概率顿悟获得大量修为。"

msgid "meditate_requirements"
msgstr "需要佛门道统"

msgid "Your orthodoxy does not support Zen Meditation."
msgstr "您的道统不支持禅定。"

msgid "{avatar} begins Zen Meditation."
msgstr "{avatar} 开始坐禅入定。"

msgid ""
"{avatar} had an epiphany during meditation! Cultivation increased "
"significantly (+{exp})."
msgstr "{avatar} 在禅定中顿悟了！修为大增 (+{exp})。"

msgid "{avatar} completed meditation with a peaceful mind."
msgstr "{avatar} 完成了禅定，心境平和。"

# Action: Educate
msgid "educate_action_name"
msgstr "教化"

msgid "educate_description"
msgstr "儒门修炼，在城市中教化世人，积累功德气运。"

msgid "educate_requirements"
msgstr "需要儒家道统，且身处城市"

msgid "Your orthodoxy does not support Confucian Education."
msgstr "您的道统不支持教化世人。"

msgid "Must be in a City to educate people."
msgstr "必须在城市中才能教化世人。"

msgid "{avatar} begins educating people in {city}."
msgstr "{avatar} 开始在 {city} 教化百姓。"

msgid "{avatar} finished educating people. Merit accumulated (+{exp})."
msgstr "{avatar} 完成了教化，功德积累 (+{exp})。"

msgid "The prosperity of {city} has increased due to {avatar}'s teachings."
msgstr "因 {avatar} 的教化，{city} 的繁荣度提升了。"

# Action: Temper
msgid "temper_action_name"
msgstr "打熬"

msgid "temper_description"
msgstr "通过极限锻炼打熬肉身气力，不假外求，不食灵气。"

msgid "temper_requirements"
msgstr "需要武道道统"

msgid "Your orthodoxy does not support Body Tempering."
msgstr "您的道统不支持打熬肉身。"

msgid "{avatar} begins tempering body strength"
msgstr "{avatar} 开始打熬气力"

msgid "action_temper_short_name"
msgstr "打熬"
