# Battle messages
msgid ""
"{winner} defeated {loser}, dealing {damage} damage. {loser} was fatally "
"wounded and perished."
msgstr "{winner} 戰勝了 {loser}，造成 {damage} 點傷害。{loser} 遭受重創，當場隕落。"

msgid ""
"{winner} defeated {loser}. {loser} took {loser_dmg} damage, {winner} also "
"took {winner_dmg} damage."
msgstr ""
"{winner} 戰勝了 {loser}，{loser} 受傷 {loser_dmg} 點，{winner} 也受傷 {winner_dmg} 點。"

# Battle - action descriptions
msgid "defeated"
msgstr "戰勝了"

msgid "{p1} and {p2} gave birth to a {gender} named {child}."
msgstr "{p1} 與 {p2} 誕下一{gender}，取名為 {child}。"

msgid "son"
msgstr "子"

msgid "daughter"
msgstr "女"
